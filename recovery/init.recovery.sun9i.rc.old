on init
    export EXTERNAL_STORAGE /external_sd

    insmod /nand.ko
    insmod /sw-device.ko debug_mask=0xff ctp_mask=1
    insmod /disp.ko
    insmod /lcd.ko
    insmod /gt9xxnew_ts.ko

    mkdir /external_sd
    mkdir /bootloader
    exec /sbin/busybox sh /sbin/init_partition.sh

service console /sbin/sh
    console
    disabled
    user shell
    group log

service adbd /sbin/adbd recovery
    socket adbd stream 660 system system

